class nested_loop
{
    public static void main(String[] args) 
    {
       int p,q;
       for(p=1; p<=9;p++)
       {
           for(q=1; q<=p;q++)
           {
               System.out.print("@ ");
           }
           System.out.println(" ");
       }
    }
}
// the outer for loop(with variable p) controls the number of rows whereas the inner for loop(with variable q) controls the number of column.
